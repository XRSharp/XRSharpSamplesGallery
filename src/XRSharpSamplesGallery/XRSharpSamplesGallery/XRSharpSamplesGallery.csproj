<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <EnableDefaultItems>false</EnableDefaultItems>
    <DefineSilverlight>true</DefineSilverlight>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="XRSharp" Version="0.1.0-*" />
  </ItemGroup>

  <ItemGroup>
    <ApplicationDefinition Include="App.xaml">
      <Generator>MSBuild:Compile</Generator>
    </ApplicationDefinition>
    <Page Include="MainPage.xaml">
      <Generator>MSBuild:Compile</Generator>
    </Page>
    <Page Include="Menu\For Desktop and Mobile\Menu2D_Styles.xaml">
      <Generator>MSBuild:Compile</Generator>
    </Page>
    <Page Include="Menu\For AR and VR\Menu3D.xaml">
      <SubType>Designer</SubType>
      <Generator>MSBuild:Compile</Generator>
    </Page>
    <Page Include="Menu\For Desktop and Mobile\Menu2D.xaml">
      <Generator>MSBuild:Compile</Generator>
    </Page>
    <Page Include="Other\Environment3D.xaml">
      <Generator>MSBuild:Compile</Generator>
    </Page>
    <Page Include="Samples\Calculator3D.xaml">
      <Generator>MSBuild:Compile</Generator>
    </Page>
    <Page Include="Samples\Charts.xaml">
      <SubType>Designer</SubType>
      <Generator>MSBuild:Compile</Generator>
    </Page>
    <Page Include="Samples\Welcome.xaml">
      <Generator>MSBuild:Compile</Generator>
    </Page>
  </ItemGroup>

  <ItemGroup>
    <Compile Include="App.xaml.cs" />
    <Compile Include="MainPage.xaml.cs" />
    <Compile Include="Menu\For AR and VR\Menu3D.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Include="Menu\For Desktop and Mobile\Menu2D.xaml.cs" />
    <Compile Include="Menu\MenuItems.cs" />
    <Compile Include="Menu\MenuItem.cs" />
    <Compile Include="Other\Environment3D.xaml.cs" />
    <Compile Include="Other\SampleAttachedProperties.cs" />
    <Compile Include="Samples\Calculator3D.xaml.cs" />
    <Compile Include="Samples\Charts.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Include="Samples\Welcome.xaml.cs" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Other\Assets\room.glb" />
    <Content Include="Menu\For Desktop and Mobile\BackgroundOverlay.png" />
    <Content Include="Other\Assets\room-license.txt" />
    <Content Include="Thumbnails\Calculator3D.jpg" />
    <Content Include="Thumbnails\Welcome.jpg" />    
  </ItemGroup>

  <ItemGroup>
    <UpToDateCheckInput Remove="Examples\**" />
  </ItemGroup>

  <ItemGroup>
    <UpToDateCheckInput Remove="Menu\Menu2D_When_Desktop_Mobile.xaml" />
  </ItemGroup>

  <ItemGroup>
    <UpToDateCheckInput Remove="Menu\Menu3D_When_VR_AR.xaml" />
  </ItemGroup>

  <ItemGroup>
    <UpToDateCheckInput Remove="Menu\Menu2D_Styles.xaml" />
  </ItemGroup>

  <ItemGroup>
    <UpToDateCheckInput Remove="Menu\For Desktop and Mobile\Menu2D.xaml" />
  </ItemGroup>

  <ItemGroup>
    <UpToDateCheckInput Remove="Menu\For AR and VR\Menu3D.xaml" />
  </ItemGroup>

  <ItemGroup>
    <UpToDateCheckInput Remove="Menu\For Desktop and Mobile\Menu2D_Styles.xaml" />
  </ItemGroup>

  <ItemGroup>
    <UpToDateCheckInput Remove="Samples\Welcome.xaml" />
  </ItemGroup>

  <ItemGroup>
    <UpToDateCheckInput Remove="Samples\Charts.xaml" />
  </ItemGroup>

</Project>
